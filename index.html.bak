	function BRUSBOX706(){
			var popup = document.getElementById("popup");	
			var popname = document.getElementById("popname");
			var popdes = document.getElementById("popdes");
	popup.style.visibility = "visible";
	popup.style.overflow = "auto";
	popname.textContent = " BRUSBOX 70-6";
	popdes.textContent = "Система BRUSBOX 70-6 — новейшее достижение в разработке энергосберегающих профильных систем от компании «БРУСБОКС». Высокотехнологичные системы данной серии имеют увеличенную монтажную глубину — 70мм и максимально возможное для данной глубины количество камер – 6, благодаря чему система BRUSBOX 70-6 обеспечивает высокие показатели тепло- и звукоизоляции оконных конструкций и является лидером в премиум-сегменте оконных профилей. Этот высококачественный морозостойкий профиль предназначен для строительства элитного жилья и коттеджей, а также является идеальным решением для использования в самых северных регионах, наиболее требовательных в вопросах теплосбережения. Благодаря совокупности повышенных энергосберегающих свойств, высокого качества исполнения и элегантного внешнего вида, система BRUSBOX 70-6 способна удовлетворить запросы даже самых притязательных потребителей. Технические особенности профилей системы «BRUSBOX 70-6»: конструктивная ширина — 70 мм, количество камер — 6 шт, толщина лицевых стенок — 3 мм, максимальная толщины заполнения — 40 мм, сопротивление теплопередаче — 1,02 м2С/Вт (без армирования).";
		}
		
	/*BRUSBOX-AERO*/
		
		function BRUSBOXAERO(){
			var popup = document.getElementById("popup");	
			var popname = document.getElementById("popname");
			var popdes = document.getElementById("popdes");
	popup.style.visibility = "visible";
	popup.style.overflow = "auto";
	popname.textContent = "BRUSBOX AERO";
